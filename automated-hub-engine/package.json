{
  "name": "automated-hub-engine",
  "version": "1.0.0",
  "description": "Enterprise Onboarding & Campaign Orchestration That Actually Works",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:e2e": "cypress run",
    "test:performance": "artillery run tests/performance/load-test.yml",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.js'",
    "setup": "node scripts/setup.js",
    "build": "webpack --mode=production",
    "docker:build": "docker build -t automated-hub-engine .",
    "docker:run": "docker-compose up -d",
    "k8s:deploy": "kubectl apply -f deployment/kubernetes/",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "docs": "jsdoc -c jsdoc.conf.js"
  },
  "keywords": [
    "user-activation",
    "onboarding",
    "campaign-orchestration",
    "saas",
    "automation",
    "enterprise",
    "behavioral-analytics",
    "churn-prediction"
  ],
  "author": "Automated Hub Engine Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "redis": "^4.6.10",
    "bull": "^4.12.2",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "@sentry/node": "^7.81.1",
    "stripe": "^14.7.0",
    "@sendgrid/mail": "^8.1.0",
    "intercom-client": "^2.11.0",
    "@slack/web-api": "^6.9.0",
    "segment-node": "^2.0.0",
    "prometheus-client": "^15.0.0",
    "graphql": "^16.8.1",
    "apollo-server-express": "^3.12.1",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "node-cron": "^3.0.3",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "cypress": "^13.6.1",
    "artillery": "^2.0.3",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "jsdoc": "^4.0.2",
    "@types/node": "^20.10.3",
    "concurrently": "^8.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/config/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/automated-hub-engine.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/automated-hub-engine/issues",
    "email": "support@automated-hub-engine.com"
  },
  "homepage": "https://automated-hub-engine.com",
  "funding": {
    "type": "enterprise",
    "url": "https://automated-hub-engine.com/enterprise"
  }
}