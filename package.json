{
  "name": "app-marketing-automation-hub",
  "version": "1.0.0",
  "description": "Multi-agent marketing automation system for app launches with email sequences, social media integration, and analytics tracking",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "validate-content": "node scripts/validate-content-schemas.js",
    "setup-apis": "node scripts/setup-api-connections.js",
    "deploy-sequences": "node scripts/deploy-email-sequences.js",
    "deploy-untrapd-automation": "node src/automation/mailchimp-untrapd-deployer.js",
    "optimize-etsy-shop": "node src/automation/etsy-optimization-deployer.js",
    "test-mailchimp": "node -e \"require('./src/services/providers/mailchimp-service.js'); console.log('Mailchimp service loaded successfully');\"",
    "test:integration": "jest --config jest.integration.config.js"
  },
  "keywords": [
    "marketing-automation",
    "email-marketing", 
    "social-media-api",
    "app-launch",
    "multi-agent-development"
  ],
  "author": "Agent B - API Integration Specialist",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "rate-limiter-flexible": "^2.4.2",
    "winston": "^3.8.2",
    "mailchimp-api-v3": "^1.15.0",
    "node-fetch": "^3.3.1",
    "@sendgrid/mail": "^7.7.0",
    "node-cron": "^3.0.2",
    "bull": "^4.10.4",
    "redis": "^4.6.5",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0",
    "joi": "^17.9.1",
    "axios": "^1.4.0",
    "twitter-api-v2": "^1.15.0",
    "handlebars": "^4.7.7",
    "moment": "^2.29.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "jsonschema": "^1.4.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/app-marketing-automation-hub.git"
  }
}